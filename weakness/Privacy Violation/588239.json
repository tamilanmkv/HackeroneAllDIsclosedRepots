ers":[],"assignable_team_member_groups":[]},"activities":[{"id":4906287,"is_internal":false,"editable":false,"type":"Activities::Comment","message":"PoC-binary.jar binds port 8000. When accessed there, it determines if the Tor Browser is in English. This is a simple example, but in practice it can accurately read the language used by the user.\n\nIf this is valid security issue, I want t-shirt as reward.","automated_response":false,"created_at":"2019-05-23T01:23:48.124Z","updated_at":"2019-05-23T01:23:48.124Z","actor":{"username":"ryotak","cleared":false,"url":"/ryotak","profile_picture_urls":{"medium":"https://profile-photos.hackerone-user-content.com/variants/Nb79aqy3v6SyMh8rwZZh43cu/fd4ecbe1e3e98c343c7717114485ace2f830f9253103d3cfa7502059868c516c"},"hackerone_triager":false,"hackerone_employee":null},"genius_execution_id":null,"team_handle":"torproject","actor_is_team_member":false,"actor_is_concealed_member":false},{"id":4906394,"is_internal":false,"editable":false,"type":"Activities::Comment","message":"If you don't want to run PoC server locally, you can use my test server:\nhttp://51.38.74.11:8000","automated_response":false,"created_at":"2019-05-23T02:38:26.161Z","updated_at":"2019-05-23T02:38:26.161Z","actor":{"username":"ryotak","cleared":false,"url":"/ryotak","profile_picture_urls":{"medium":"https://profile-photos.hackerone-user-content.com/variants/Nb79aqy3v6SyMh8rwZZh43cu/fd4ecbe1e3e98c343c7717114485ace2f830f9253103d3cfa7502059868c516c"},"hackerone_triager":false,"hackerone_employee":null},"genius_execution_id":null,"team_handle":"torproject","actor_is_team_member":false,"actor_is_concealed_member":false},{"id":4906529,"is_internal":false,"editable":false,"type":"Activities::Comment","message":"I was wrong in \"details\". it caused by a link tag that added by Tor Browser in zero-length page.\nHttp status code won't affect this vulnerability.","automated_response":false,"created_at":"2019-05-23T03:39:47.484Z","updated_at":"2019-05-23T03:39:47.484Z","actor":{"username":"ryotak","cleared":false,"url":"/ryotak","profile_picture_urls":{"medium":"https://profile-photos.hackerone-user-content.com/variants/Nb79aqy3v6SyMh8rwZZh43cu/fd4ecbe1e3e98c343c7717114485ace2f830f9253103d3cfa7502059868c516c"},"hackerone_triager":false,"hackerone_employee":null},"genius_execution_id":null,"team_handle":"torproject","actor_is_team_member":false,"actor_is_concealed_member":false},{"id":4920311,"is_internal":false,"editable":false,"type":"Activities::Comment","message":"Hi @ryotak,\n\nThank you for your submission. Your report is currently being reviewed and the HackerOne triage team will get back to you once there is additional information to share.\n\nKind regards,\n@orangeband ","automated_response":false,"created_at":"2019-05-24T20:30:46.260Z","updated_at":"2019-05-24T20:30:46.260Z","actor":{"username":"orangeband","cleared":false,"url":"/orangeband","profile_picture_urls":{"medium":"https://profile-photos.hackerone-user-content.com/variants/000/390/540/b55c564a8a6f8099d28ea9afa41d197ad0031fc3_original.jpg/fd4ecbe1e3e98c343c7717114485ace2f830f9253103d3cfa7502059868c516c"},"hackerone_triager":true,"hackerone_employee":true},"genius_execution_id":null,"team_handle":"torproject","actor_is_team_member":false,"actor_is_concealed_member":false},{"id":4920392,"is_internal":false,"editable":false,"type":"Activities::BugNeedsMoreInfo","message":"Hi @ryotak \n\nI saw in your PoC video that you are only able to specify english and non-english as the proof of concept. Are you able to determine that actual language from non english versions?\n\nAlso, is there a way to change the languages on the browser or is it based on the initial installation? \n\nThanks,\n@orangeband ","automated_response":false,"created_at":"2019-05-24T20:58:49.438Z","updated_at":"2019-05-24T20:58:49.438Z","actor":{"username":"orangeband","cleared":false,"url":"/orangeband","profile_picture_urls":{"medium":"https://profile-photos.hackerone-user-content.com/variants/000/390/5